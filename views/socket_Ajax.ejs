<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <title>hello</title>
    <script src="jquery.js"></script>
    <script src="socket.io/socket.io.js"></script>
    <script>
        var socket = io("http://localhost:3000");
        socket.on("serversentdata", function (data) {
            $("#nd").html(data);
        });
        $(document).ready(function () {
            $("#input2").click(function () {
                var use = $("#username").val();
                var pass = $("#password").val();
                socket.emit("clientsentdata", use + "/" + pass);
            });

        });

        function he() {
            $.ajax({
                type: 'GET',
                url: '/he/' + "hihi",
                dataType: 'json'
            })
                .done(function (data) {
                    data.forEach(function (item) {
                        $('#nd').append("<h2>" + item.id + "</h2>");
                    });

                });
        }
    </script>
</head>

<body>


    <input type="text" id="username">

    <input type="password" id="password">

    <input id="input2" type="submit" value="Login">

    <button id="132" onclick="he()">Meo</button>

    <div id="nd"></div>
    </div>
</body>

</html>